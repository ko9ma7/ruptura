{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET: Launch (External)",
      "presentation": {
        "group": "ruptura"
      },
      "type": "coreclr",
      "request": "launch",
      "stopAtEntry": true,
      "suppressJITOptimizations": true,
      "justMyCode": false,
      "symbolOptions": {
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      },
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src/sample",
      "program": "${workspaceFolder}/src/sample/bin/Debug/sample.dll"
    },
    {
      "name": ".NET: Launch (Integrated)",
      "presentation": {
        "group": "ruptura"
      },
      "type": "coreclr",
      "request": "launch",
      "stopAtEntry": true,
      "suppressJITOptimizations": true,
      "justMyCode": false,
      "symbolOptions": {
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      },
      "console": "externalTerminal",
      "cwd": "${workspaceFolder}/src/sample",
      "program": "${workspaceFolder}/src/sample/bin/Debug/sample.dll"
    },
    {
      "name": ".NET: Attach",
      "presentation": {
        "group": "ruptura"
      },
      "type": "coreclr",
      "request": "attach",
      "suppressJITOptimizations": true,
      "justMyCode": false,
      "symbolOptions": {
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      },
      "processId": "${command:pickProcess}"
    }
  ]
}
